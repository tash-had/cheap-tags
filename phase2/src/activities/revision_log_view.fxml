<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="activities.RevisionLogViewController">
    <TableView fx:id="revisionLog" prefHeight="367.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="currentName" prefWidth="200.0" text="Current Name"/>
            <TableColumn fx:id="oldName" prefWidth="200.0" text="Old Name"/>
            <TableColumn fx:id="timeStamp" prefWidth="200.0" text="Time Stamp"/>
        </columns>
    </TableView>
    <TextField fx:id="revisionHistorySearchBar" layoutY="366.0" prefHeight="34.0" prefWidth="400.0"/>
    <Button fx:id="revertTo" layoutX="400.0" layoutY="366.0" mnemonicParsing="false" onAction="#setRevertTo"
            prefHeight="34.0" prefWidth="200.0" text="Revert to"/>
</AnchorPane>
